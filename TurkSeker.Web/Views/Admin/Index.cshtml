@model IEnumerable<TurkSeker.Dal.Db.Kullanici>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<div class="panel panel-primary">

    <div class="panel-heading">
        Users

    </div>
    <table class="table table-striped">
        <tr>

        
            <th>Kullanıcı</th>

        </tr>

        @if (Model.Count() == 0)
        {
            <tr>
                <td colspan="2" class="text-center"> Kullanıcı Yok </td>



            </tr>
        }
        else
        {

            foreach (var item in Model)
            {

                <tr> 
                    <td>@item.UserName</td> 
                </tr>

            } 
        } 
    </table>


</div>

